<AnchorPane fx:id="mainWindowPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #0DFF00;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainWindowController">
